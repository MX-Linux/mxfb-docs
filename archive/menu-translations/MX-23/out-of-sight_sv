[begin] (Dold) 
    [exec] (Stäng docka) { killall wmalauncher               }
    [exec] (Stänga ett fönster) { xkill                             }
    [separator]
    [exec] (Växla conky) { conkytoggle.sh                    }
    [exec] (Växla iDesk) { idesktoggle idesk                 }
    [exec] (Växla ikoner) { idesktoggle icons                 }
    [exec] (Växla auto-dold docka) { toggledock }
    [exec] (Växla auto-dolt verktygsfält) { toggletoolbar}
     [exec] (Växla auto-dold panel) { togglepanel}
    [exec] (Växla skärmar) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Avaktivera standard-docka) { killall wmalauncher && dfltdck_kill }
[end]
