[begin] () 
    [exec] (Kill dock) { killall wmalauncher               }
    [exec] (Tapa ikkuna) { xkill                             }
    [separator]
    [exec] (Toggle conky) { conkytoggle.sh                    }
    [exec] (Vaihda iDesk) { idesktoggle idesk                 }
    [exec] (Vaihda kuvakkeita) { idesktoggle icons                 }
    [exec] (Toggle autohide dock) { toggledock }
    [exec] (Toggle autohide toolbar) { toggletoolbar}
     [exec] (Toggle autohide panel) { togglepanel}
    [exec] (Vaihda näyttöjä) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Poista oletustelakka käytöstä) { killall wmalauncher && dfltdck_kill }
[end]
