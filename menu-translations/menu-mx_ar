#the default versions of the submenus are located in /usr/share/mxflux/menu

[begin] (Fluxbox)
		[submenu] (All Apps)
            [include] (~/.fluxbox/submenus/full_menu)
            [separator]
            [exec] (Update Menu)  {/usr/bin/mxfb-menu-generator}
   [end]
	[submenu] () 
			[include] (~/.fluxbox/submenus/recent_files_menu)
			[separator]
			[exec] () {~/.fluxbox/scripts/recentfiles-fbmenu}
		[end]
	[separator]
    [exec] (المتصفحالإلكتروني) {sensible-browser}
    [exec] (مدير الملفات) {exo-open $HOME/.fluxbox} 
    [exec] (مساعدة) {exo-open /usr/share/mxflux/help}
    [exec] (موسيقى) {deadbeef}
    [exec] (تشغيل) {fbrun}
    [exec] () {custom-toolbox /etc/custom-toolbox/mxfb-settings.list} 
    [exec] (طرفية) {roxterm} 
    [separator]
    [submenu] (مظهر)
    [include] (~/.fluxbox/submenus/appearance)
    [end]
    [submenu] (إعدادات)
    [include] (~/.fluxbox/submenus/settings)
    [end]
    [submenu] ()
        [include] (~/.fluxbox/submenus/out-of-sight)
    [end]
    [submenu] (مغادرة)
        [exec] (تحديث) { fluxbox-remote restart; idesktoggle idesk refresh }
        [exec] (تعليق) {sudo 'pm-suspend'}
        [exit] (تسجيل) 
        [exec] (إعادة) {sudo /sbin/reboot} 
        [exec] (إيقاف) {sudo /sbin/halt}
    [end]
[end]
