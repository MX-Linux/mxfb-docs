#the default versions of the submenus are located in /usr/share/mxflux/menu

[begin] (Fluxbox)
	[exec] (全てのアプリ) {rofi -show drun}
	[submenu] (最近のファイル) 
			[include] (~/.fluxbox/submenus/recent_files_menu)
			[separator]
			[exec] (更新リスト) {~/.fluxbox/scripts/recentfiles-fbmenu}
		[end]
	[separator]
    [exec] (ブラウザ) {sensible-browser}
    [exec] (ファイルマネージャ) {thunar} 
    [exec] (ヘルプ) {thunar /usr/share/mxflux/help/}
    [exec] (ミュージック) {deadbeef}
    [exec] (実行) {fbrun}
    [exec] (設定マネージャ) {custom-toolbox /etc/custom-toolbox/mxfb-settings.list} 
    [exec] (ターミナル) {xfce4-terminal} 
        [separator]
    [submenu] (外観)
    [include] (/usr/share/mxflux/menu/menu-translations/appearance_ja)
    [end]
    [submenu] (設定)
    [include] (/usr/share/mxflux/menu/menu-translations/settings_ja)
    [end]
    [submenu] (視界の外)
        [include] (/usr/share/mxflux/menu/menu-translations/out-of-sight_ja)
    [end]
    [submenu] (終了)
        [exec] (リフレッシュ) { fluxbox-remote restart; idesktoggle idesk refresh }
        [exec] (サスペンド) {sudo 'pm-suspend'}
        [exit] (ログアウト) 
        [exec] (シャットダウン) {sudo /sbin/halt}
    [end]
[end]
