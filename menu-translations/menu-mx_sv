#the default versions of the submenus are located in /usr/share/mxflux/menu

[begin] (Fluxbox)
	[exec] (Alla program) {rofi -show drun}
	[submenu] (Senaste filer) 
			[include] (~/.fluxbox/submenus/recent_files_menu)
			[separator]
			[exec] (Uppdateringslista) {~/.fluxbox/scripts/recentfiles-fbmenu}
		[end]
	[separator]
    [exec] (Webbläsare) {sensible-browser}
    [exec] (Filhanterare) {thunar} 
    [exec] (Hjälp) {thunar /usr/share/mxflux/help/}
    [exec] (Musik) {deadbeef}
    [exec] (Kör) {fbrun}
    [exec] (Inställningshanterare) {custom-toolbox /etc/custom-toolbox/mxfb-settings.list} 
    [exec] (Terminal) {xfce4-terminal} 
        [separator]
    [submenu] (Utseende)
    [include] (/usr/share/mxflux/menu/menu-translations/appearance_sv)
    [end]
    [submenu] (Inställningar)
    [include] (/usr/share/mxflux/menu/menu-translations/settings_sv)
    [end]
    [submenu] (Dold)
        [include] (/usr/share/mxflux/menu/menu-translations/out-of-sight_sv)
    [end]
    [submenu] (Avsluta)
        [exec] (Förnya) { fluxbox-remote restart; idesktoggle idesk refresh }
        [exec] (Vänteläge) {sudo 'pm-suspend'}
        [exit] (Logga ut) 
        [exec] (Stäng av) {sudo /sbin/halt}
    [end]
[end]
