# the submenus can be found in /usr/share/mxflux/menu/

[begin] (Fluxbox)
	[submenu] (All Apps)
            [include] (~/.fluxbox/submenus/full_menu)
            [separator]
            [exec] (Update Menu)  {/usr/bin/mxfb-menu-generator}
   [end]
	[submenu] (Senaste filer) 
			[include] (~/.fluxbox/submenus/recent_files_menu)
			[separator]
			[exec] (Uppdatera lista) {~/.fluxbox/scripts/recentfiles-fbmenu}
		[end]
	[separator]
    [exec] (Webbläsare) {sensible-browser}
    [exec] (E-post) {xdg-email}
    [exec] (Filhanterare) {thunar $HOME/.fluxbox} 
    [exec] (Hjälp) {thunar /usr/share/mxflux/help}
    [exec] (Musik) {clementine}
    [exec] (Kör) {fbrun} 
    [exec] (Terminal) {xfce4-terminal} 
    [separator]
    [submenu] (Utseende)
    [include] (~/.fluxbox/submenus/appearance)
    [end]
    [submenu] (Inställningar)
    [include] (~/.fluxbox/submenus/settings)
    [end]
    [submenu] (Dold)
        [include] (~/.fluxbox/submenus/out-of-sight)
    [end]
    [submenu] (Avsluta)
        [exec] (Förnya) { fluxbox-remote restart; idesktoggle idesk refresh }
        [exec] (Upphör) {sudo 'pm-suspend'}
        [exit] (Logga ut) 
        [exec] (Starta om) {sudo /sbin/reboot} 
        [exec] (Stäng av) {sudo /sbin/halt}
    [end]
[end]
