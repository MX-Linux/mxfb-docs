[begin] () 
    [exec] (Док за убиване) { killall wmalauncher               }
    [exec] (Избиване на прозорец) { xkill                             }
    [separator]
    [exec] (Превключване на conky) { conkytoggle.sh                    }
    [exec] (Превключване на iDesk) { idesktoggle idesk                 }
    [exec] (Превключване на икони) { idesktoggle icons                 }
    [exec] (Превключване на автоматично скриване на док) { toggledock }
    [exec] (Превключване на автоматичното скриване на лентата с инструменти) { toggletoolbar}
     [exec] (Превключване на автоматично скриване на панела) { toggletint2}
    [exec] (Превключване на мониторите) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Деактивиране на док по подразбиране) { killall wmalauncher && dfltdck_kill }
[end]
