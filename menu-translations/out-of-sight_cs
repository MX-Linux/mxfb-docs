[begin] () 
    [exec] (Kill dock) { killall wmalauncher               }
    [exec] (Zabít okno) { xkill                             }
    [separator]
    [exec] (Přepnout conky) { conkytoggle.sh                    }
    [exec] (Přepnout iDesk) { idesktoggle idesk                 }
    [exec] (Přepnout ikony) { idesktoggle icons                 }
    [exec] (Přepnout automatické skrytí doku) { $HOME/.fluxbox/scripts/toggledock }
    [exec] (Přepnout automatické skrytí panelu nástrojů) {$HOME/.fluxbox/scripts/toggletoolbar}
     [exec] (Přepnout automatické skrytí panelu) {$HOME/.fluxbox/scripts/toggletint2}
    [exec] (Přepnout monitory) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Zakázat výchozí dok) { killall wmalauncher && $HOME/.fluxbox/scripts/dfltdck_kill }
[end]
