[begin] (Verstecken) 
    [exec] (Dock beenden) { killall wmalauncher               }
    [exec] (Fenster beenden) { xkill                             }
    [separator]
    [exec] (Conky an/aus) { conkytoggle.sh                    }
    [exec] (iDesk an/aus) { idesktoggle idesk                 }
    [exec] (Symbole an/aus) { idesktoggle icons                 }
    [exec] (Automatisches Ausblenden des Docks an/aus) { toggledock }
    [exec] (Automatisches Ausblenden der Werkzeugleiste an/aus) { toggletoolbar}
    [exec] (Automatisches Ausblenden des Panel an/aus) { togglepanel}
    [exec] (System-Monitor umschalten) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Standard-Dock deaktivieren) { killall wmalauncher && dfltdck_kill }
[end]
