[begin] (No visible) 
    [exec] (Matar un dock) { killall wmalauncher               }
    [exec] (Matar una ventana) { xkill                             }
    [separator]
    [exec] (Alternar conky) { conkytoggle.sh                    }
    [exec] (Alternar iDesk) { idesktoggle idesk                 }
    [exec] (Alternar iconos) { idesktoggle icons                 }
    [exec] (Alternar auto ocultar dock) { toggledock }
    [exec] (Alternar auto ocultar barra de herramientas) { toggletoolbar}
     [exec] (Alternar auto ocultar panel) { togglepanel}
    [exec] (Alternar monitores) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Deshabilitar dock predeterminado) { killall wmalauncher && dfltdck_kill }
[end]
