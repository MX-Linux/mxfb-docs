[begin] () 
    [exec] (Kill dock) { killall wmalauncher               }
    [exec] (Tapa ikkuna) { xkill                             }
    [separator]
    [exec] (Toggle conky) { conkytoggle.sh                    }
    [exec] (Vaihda iDesk) { idesktoggle idesk                 }
    [exec] (Vaihda kuvakkeita) { idesktoggle icons                 }
    [exec] (Toggle autohide dock) { $HOME/.fluxbox/scripts/toggledock }
    [exec] (Toggle autohide toolbar) {$HOME/.fluxbox/scripts/toggletoolbar}
     [exec] (Toggle autohide panel) {$HOME/.fluxbox/scripts/toggletint2}
    [exec] (Vaihda näyttöjä) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Poista oletustelakka käytöstä) { killall wmalauncher && $HOME/.fluxbox/scripts/dfltdck_kill }
[end]
