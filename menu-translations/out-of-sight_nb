[begin] (Ute av syne) 
    [exec] (Tvangsavslutt samlevindu) { killall wmalauncher               }
    [exec] (Tvangsavslutt vindu) { xkill                             }
    [separator]
    [exec] (Veksle conky) { conkytoggle.sh                    }
    [exec] (Veksle iDesk) { idesktoggle idesk                 }
    [exec] (Veksle ikoner) { idesktoggle icons                 }
    [exec] (Veksle autoskjuling av samlevindu) { toggledock }
    [exec] (Veksle autoskjuling av verktøylinje) { toggletoolbar}
     [exec] (Veksle autoskjuling av panel) { toggletint2}
    [exec] (Veksle skjermer) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Slå av forvalgt samlevindu) { killall wmalauncher && dfltdck_kill }
[end]
