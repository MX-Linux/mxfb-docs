[begin] (Uit het zicht) 
    [exec] (Beëindig dock) { killall wmalauncher               }
    [exec] (Beëindig venster) { xkill                             }
    [separator]
    [exec] (Wissel conky) { conkytoggle.sh                    }
    [exec] (Wissel iDesk) { idesktoggle idesk                 }
    [exec] (Wissel iconen) { idesktoggle icons                 }
    [exec] (Wissel automatisch dock verbergen) { toggledock }
    [exec] (Wissel automatisch gereedschapsbalk verbergen) { toggletoolbar}
     [exec] (Wissel automatisch gereedschapsbalk verbergen) { toggletint2}
    [exec] (Wissel beeldschermen) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Standaard dock uitschakelen) { killall wmalauncher && dfltdck_kill }
[end]
