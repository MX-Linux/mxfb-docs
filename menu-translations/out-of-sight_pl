[begin] (Poza polem widzenia) 
    [exec] (Usuń dok) { killall wmalauncher               }
    [exec] (Zakończ proces dla wybranego okna) { xkill                             }
    [separator]
    [exec] (Pokaż/ukryj conky) { conkytoggle.sh                    }
    [exec] (Pokaż/ukryj iDesk) { idesktoggle idesk                 }
    [exec] (Pokaż/ukryj ikony) { idesktoggle icons                 }
    [exec] (Przełącz automatyczne ukrywanie doku) { toggledock }
    [exec] (Przełącz automatyczne ukrywanie paska narzędz) { toggletoolbar}
     [exec] (Przełącz automatyczne ukrywanie panelu) { toggletint2}
    [exec] (Przełącz monitory) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Wyłącz domyślny dok) { killall wmalauncher && dfltdck_kill }
[end]
