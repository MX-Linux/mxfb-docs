[begin] () 
    [exec] (Убить док) { killall wmalauncher               }
    [exec] (Убить окно) { xkill                             }
    [separator]
    [exec] (Переключить conky) { conkytoggle.sh                    }
    [exec] (Переключить iDesk) { idesktoggle idesk                 }
    [exec] (Переключить значки) { idesktoggle icons                 }
    [exec] (Переключить автоскрытие дока) { $HOME/.fluxbox/scripts/toggledock }
    [exec] (Переключить автоскрытие панели инструментов) {$HOME/.fluxbox/scripts/toggletoolbar}
     [exec] (Переключить автоскрытие панели) {$HOME/.fluxbox/scripts/toggletint2}
    [exec] (Переключить мониторы) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Отключить док по умолчанию) { killall wmalauncher && $HOME/.fluxbox/scripts/dfltdck_kill }
[end]
