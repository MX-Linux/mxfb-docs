[begin] () 
    [exec] (Убить док) { killall wmalauncher               }
    [exec] (Убить окно) { xkill                             }
    [separator]
    [exec] (Переключить conky) { conkytoggle.sh                    }
    [exec] (Переключить iDesk) { idesktoggle idesk                 }
    [exec] (Переключить значки) { idesktoggle icons                 }
    [exec] (Переключить автоскрытие дока) { toggledock }
    [exec] (Переключить автоскрытие панели инструментов) { toggletoolbar}
     [exec] (Переключить автоскрытие панели) { toggletint2}
    [exec] (Переключить мониторы) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Отключить док по умолчанию) { killall wmalauncher && dfltdck_kill }
[end]
