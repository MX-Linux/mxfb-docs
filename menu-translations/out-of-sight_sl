[begin] (Izven vidnega) 
    [exec] (Ukini sidrišče) { killall wmalauncher               }
    [exec] (Ukini okno) { xkill                             }
    [separator]
    [exec] (Preklopi Conky) { conkytoggle.sh                    }
    [exec] (Preklopi iDesk) { idesktoggle idesk                 }
    [exec] (Preklopi ikone) { idesktoggle icons                 }
    [exec] (Preklopi samodejno skirvanje sidrišča) { toggledock }
    [exec] (Preklopi samodejno skrivanje orodne vrstice) { toggletoolbar}
     [exec] (Preklopi samodejno skirvanje panoja) { toggletint2}
    [exec] (Preklop zaslonov) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Onemogoči privzeto sidrišče) { killall wmalauncher && dfltdck_kill }
[end]
