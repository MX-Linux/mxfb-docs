[begin] (Dold) 
    [exec] (Stänga docka) { killall wmalauncher               }
    [exec] (Stänga ett fönster) { xkill                             }
    [separator]
    [exec] (Växla conky) { conkytoggle.sh                    }
    [exec] (Växla iDesk) { idesktoggle idesk                 }
    [exec] (Växla ikoner) { idesktoggle icons                 }
    [exec] (Växla auto-dold docka) { $HOME/.fluxbox/scripts/toggledock }
    [exec] (Växla auto-dolt verktygsfält) {$HOME/.fluxbox/scripts/toggletoolbar}
    [exec] (Växla skärmar) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Avaktivera standard-docka) { killall wmalauncher && $HOME/.fluxbox/scripts/dfltdck_kill }
[end]
