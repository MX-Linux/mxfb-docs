           [exec] (Pengaturan) {exo-open ~/.fluxbox/submenus/settings}
           [exec] (Di luar tampilan) {exo-open ~/.fluxbox/submenus/out-of-sight}
        [end]
        [exec] (Overlay) {exo-open ~/.fluxbox/overlay}
        [exec] (Startup) {featherpad ~/.fluxbox/startup}
        [exec] (Gaya) {thunar ~/.fluxbox/styles/}
        [exec] (Panel Tint2) {mxfb-tint2-manager}
    [end]
    [submenu] (Tampilan)
