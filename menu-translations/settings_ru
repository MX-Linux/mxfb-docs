[begin] (Настройки)
    [exec] (Резервное копирование) {grsync}
    [submenu] (Конфигурационные файлы)
        [config] (Быстрая настройка)
        [exec] (Приложения) {exo-open ~/.fluxbox/apps}
        [submenu] (Инициализация) 
        	[exec] (Редактирование) {exo-open ~/.fluxbox/init}
        	[separator]
        	[exec] (Категоризация) {mxfb-sortinit}
        [end]
        [exec] (Клавиши) {exo-open ~/.fluxbox/keys}
        [submenu] (Меню) 
           [exec] (Главное) {exo-open ~/.fluxbox/menu-mx}
           [exec] (Внешний вид) {exo-open ~/.fluxbox/submenus/appearance}
           [exec] (Настройки) {exo-open ~/.fluxbox/submenus/settings}
           [exec] (Скрыть) {exo-open ~/.fluxbox/submenus/out-of-sight}
        [end]
        [exec] (Наложение) {exo-open ~/.fluxbox/overlay}
        [exec] (Запуск) {featherpad ~/.fluxbox/startup}
        [exec] (Стили) {thunar ~/.fluxbox/styles/}
        [exec] (Панель Tint2) {mxfb-tint2-manager}
    [end]
    [submenu] (Отображение)
    	[exec] (Изменить) {arandr}
    	[exec](Справка) {xdg-open /usr/share/mxflux/help/HELP_Display}
    	[exec] (Заставка) {exo-open /usr/share/doc/mxfb-accessories/help_mxfb-screensaver}
    [end]
    [submenu] (Клавиатура)
        [exec] (DE) {setxkbmap de}
        [exec] (ES) {setxkbmap es} 
        [exec] (FR) {setxkbmap fr}
        [exec] (GB) {setxkbmap gb}
        [exec] (GR) {setxkbmap gr}
        [exec] (JA) {setxkbmap ja}
        [exec] (IT) {setxkbmap it}
        [exec] (PL) {setxkbmap pl}
        [exec] (PT) {setxkbmap pt}
        [exec] (RU) {setxkbmap ru}
        [exec] (US) {setxkbmap us}
    [end]
    [submenu] (Панели инструментов)
        [exec] (Fluxbox) {toggletint2}
        [exec] (tint2) {toggletint2}
    [end]
