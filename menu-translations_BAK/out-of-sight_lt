[begin] () 
    [exec] (Nužudymo dokas) { killall wmalauncher               }
    [exec] (Nužudyti langą) { xkill                             }
    [separator]
    [exec] (Perjungti conky) { conkytoggle.sh                    }
    [exec] (Perjungti iDesk) { idesktoggle idesk                 }
    [exec] (Perjungti piktogramas) { idesktoggle icons                 }
    [exec] (Perjungti automatiškai paslėpti doką) { $HOME/.fluxbox/scripts/toggledock }
    [exec] (Perjungti automatinį įrankių juostos paslėpimą) {$HOME/.fluxbox/scripts/toggletoolbar}
     [exec] (Perjungti automatinį skydelio paslėpimą) {$HOME/.fluxbox/scripts/toggletint2}
    [exec] (Perjungti monitorius) { toggle-mx gkrellm                 }
    [separator]
    [exec] (Išjungti numatytąjį doką) { killall wmalauncher && $HOME/.fluxbox/scripts/dfltdck_kill }
[end]
