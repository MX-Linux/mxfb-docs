[begin] () 
    [exec] (杀死船坞) { killall wmalauncher               }
    [exec] (杀死一个窗口) { xkill                             }
    [separator]
    [exec] (切换康基) { conkytoggle.sh                    }
    [exec] (切换iDesk) { idesktoggle idesk                 }
    [exec] (切换图标) { idesktoggle icons                 }
    [exec] (切换自动隐藏Dock) { $HOME/.fluxbox/scripts/toggledock }
    [exec] (切换自动隐藏工具条) {$HOME/.fluxbox/scripts/toggletoolbar}
     [exec] (切换自动隐藏面板) {$HOME/.fluxbox/scripts/toggletint2}
    [exec] (切换显示器) { toggle-mx gkrellm                 }
    [separator]
    [exec] (禁用默认底座) { killall wmalauncher && $HOME/.fluxbox/scripts/dfltdck_kill }
[end]
